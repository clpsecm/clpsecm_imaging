function lines = read_clpsecm_data(dataProfile)

%-Read xls file
folderpwd = 'data/';
filename = ['clpsecm_',dataProfile.date,...
            '_S',      num2str(dataProfile.sample_number,'%03d'),...
            '_L',      num2str(dataProfile.nlines,'%02d'),...
            '_',       num2str(dataProfile.version,'%02d'),...
            '.xlsx'];
nmsrs = (dataProfile.scan_length) / (dataProfile.scan_resolution);


%-Assign lines data
lines = ScanLines(dataProfile.angles);
lines.nmeasures = nmsrs;
lines.xticks = xlsread([folderpwd,filename] ,2 ,['B2:B',num2str(nmsrs+1)]);                
for L = 1:dataProfile.nlines
    lines.current(:,nmsrs) = ...
        xlsread([folderpwd,filename] ,L+1 ,['C2:C',num2str(nmsrs+1)]);  
end

